{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "angularfirestore-deep",
    "public": "./dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/demo/**"
      },
      {
        "source": "/docs/**",
        "destination": "/docs/**"
      }
    ],
    "ignore": [
      "/angularfirestore-deep",
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
